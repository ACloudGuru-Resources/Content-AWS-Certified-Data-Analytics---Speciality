{
    "Version": "2012-10-17",
    "Statement":[
     {
        "Sid":"AllowList",
        "Action":[
            "s3:ListBucket"
        ],
        "Effect":"Allow",
        "Resource":[
            "arn:aws:s3:::<TEAM-DATA>"
        ],
        "Condition":{
           "StringLike":{
               "s3:prefix":[
                   "team/*"
               ]
           }
        }
     },
     {
       "Sid":"AllowGroupToReadWrite", 
       "Action":[
           "s3:GetObject",
           "s3:PutObject"
       ],
       "Effect":"Allow",
       "Resource":[
           "arn:aws:s3:::<TEAM-DATA>/team/*"
       ]
     }
  ]
}